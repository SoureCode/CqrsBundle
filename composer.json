{
    "name": "sourecode/cqrs-bundle",
    "type": "symfony-bundle",
    "description": "Provides cqrs integration",
    "keywords": ["symfony", "cqrs", "messenger"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Schilling",
            "email": "jason@sourecode.dev"
        }
    ],
    "require": {
        "php": "^8.0 || ^8.1",
        "doctrine/common": "^3.2",
        "sourecode/cqrs": "dev-master",
        "symfony/config": "^5.3 || ^5.4 || ^6.0 || ^6.1",
        "symfony/dependency-injection": "^5.3 || ^5.4 || ^6.0 || ^6.1",
        "symfony/http-kernel": "^5.3 || ^5.4 || ^6.0 || ^6.1",
        "symfony/uid": "^5.3 || ^5.4 || ^6.0 || ^6.1"
    },
    "require-dev": {
        "symfony/messenger": "^5.3 || ^5.4 || ^6.0 || ^6.1",
        "symfony/validator": "^5.3 || ^5.4 || ^6.0 || ^6.1",
        "symfony/framework-bundle": "^5.3 || ^5.4 || ^6.0 || ^6.1"
    },
    "autoload": {
        "psr-4": {
            "SoureCode\\Bundle\\Cqrs\\": "src"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SoureCode\\Bundle\\Cqrs\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "minimum-stability": "dev"
}
