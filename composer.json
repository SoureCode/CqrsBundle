{
    "name": "sourecode/cqrs-bundle",
    "type": "symfony-bundle",
    "description": "Provides cqrs integration",
    "keywords": [
        "symfony",
        "cqrs",
        "messenger"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Schilling",
            "email": "jason@sourecode.dev"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/http-kernel": "^5.3 || ^6.0",
        "symfony/dependency-injection": "^5.3 || ^6.0",
        "symfony/config": "^5.3 || ^6.0",
        "symfony/console": "^5.3 || ^6.0",
        "sourecode/cqrs": "dev-master"
    },
    "require-dev": {
        "nyholm/symfony-bundle-test": "dev-master",
        "symfony/validator": "^5.3 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "SoureCode\\Bundle\\Cqrs\\": "./src"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SoureCode\\Bundle\\Cqrs\\Tests\\": "./tests"
        }
    },
    "minimum-stability": "dev"
}
